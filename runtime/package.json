{
  "name": "@agentl2/runtime",
  "version": "0.1.0",
  "description": "AgentL2 Runtime - AI Agent Execution Engine",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:worker": "tsx watch src/worker/index.ts",
    "dev:listener": "tsx watch src/listener/index.ts",
    "start:api": "node dist/api/server.js",
    "start:worker": "node dist/worker/index.js",
    "start:listener": "node dist/listener/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bullmq": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jose": "^5.2.0",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.17.0",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
