# NEXT_PUBLIC_L1_BRIDGE_ADDRESS=0x...
# NEXT_PUBLIC_L1_RPC_URL=https://rpc.sepolia.org
# NEXT_PUBLIC_L1_CHAIN_ID=11155111

LOCALHOST_RPC_URL=http://localhost:8545
