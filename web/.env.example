NEXT_PUBLIC_CHAIN_ID=1337

# Contract addresses (from deploy:local or deploy script output)
NEXT_PUBLIC_REGISTRY_ADDRESS=0x5FbDB2315678afecb367f032d93F642f64180aa3
NEXT_PUBLIC_MARKETPLACE_ADDRESS=0xe7f1725E7734CE288F8367e1Bb143E90bb3F0512
NEXT_PUBLIC_BRIDGE_ADDRESS=0x9fE46736679d2D9a65F0992F2272dE9f3c7fa6e0

